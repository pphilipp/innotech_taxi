<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid
                android:color="#104c4d4f"/>
            <padding
                android:top="2dp"
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"/>
            <corners
                android:radius="1dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#ffffff"/>

            <padding
                android:top="5dp"
                android:bottom="5dp"
                android:left="4dp"
                android:right="4dp"/>
        </shape>
    </item>
</layer-list>